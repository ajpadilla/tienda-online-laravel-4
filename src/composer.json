{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"mcamara/laravel-localization": "0.14.*",
		"laravelbook/ardent": "dev-master",
		"way/generators": "2.*",
		"barryvdh/laravel-debugbar": "1.*",
		"dompdf/dompdf" : "dev-master",
		"stripe/stripe-php" : "dev-master",
		"anouar/paypalpayment": "dev-master",
		"omnipay/omnipay": "~2.0",
		"fzaninotto/faker" : "dev-master",
		"zizaco/confide": "3.2.x",
        "zizaco/entrust": "1.2.*@dev",
        "bllim/datatables": "*",
        "anahkiasen/former": "dev-develop",
		"intervention/image": "2.*",
        "intervention/imagecache": "dev-master",
        "moltin/tax": "dev-master",
        "moltin/currency": "1.0.*@dev",
        "moltin/cart": "dev-master",
        "moltin/laravel-cart": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable",
	"mcamara/laravel-localization": "0.14.*"
}
